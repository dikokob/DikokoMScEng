	isbn = {0921-0296},
	issn = {0921-0296},
	journal = {Journal of Intelligent {\&} Robotic Systems},
	keywords = {Blackwellized particle filters,Mobile robots {\textperiodcentered},Multi robot SLAM {\textperiodcentered},Rao-},
	number = {2},
	pages = {283--307},
	title = {{Simultaneous Localization and Mapping Using Rao-Blackwellized Particle Filters in Multi Robot Systems}},
	url = {http://link.springer.com/10.1007/s10846-010-9457-0},
