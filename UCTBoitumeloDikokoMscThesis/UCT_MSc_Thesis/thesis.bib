	doi = {10.1007/s11222-011-9271-y},
	file = {:C$\backslash$:/Google Drive/My Documents/Varsity/Masters/MSc-2017-2018/MSc-2017-2018 Write-up/Unsorted/An adaptive sequential Monte Carlo method for approximate.pdf:pdf},
