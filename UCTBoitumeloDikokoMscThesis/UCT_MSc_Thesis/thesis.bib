@article{Dinnissen2012,
	abstract = {Using `Simultaneous Localization and Mapping' (SLAM), mobile robots can become truly autonomous in the exploration of their environment. However, once these environments becomes too large, Multi-Robot SLAM becomes a requirement. This paper will outline how a mobile robot should decide when best to merge its maps with another robot's upon rendezvous, as opposed to doing so immediately. This decision will be based on the current status of the mapping particle filters and the current status of the environment. Using Reinforcement Learning, a model can be established and then trained upon to determine a policy capable of deciding when best to merge. This will allow the robot to incur less error during a merge compared to simply merging immediately. This policy is trained and validated using simulated mobile robot datasets.},
	author = {Dinnissen, Pierre and Givigi, Sidney N. and Schwartz, Howard M.},
	doi = {10.1109/ICSMC.2012.6377676},
	file = {:C$\backslash$:/Google Drive/My Documents/Varsity/Masters/MSc-2017-2018/MSc-2017-2018 Write-up/Papers/Map Merging/Map Merging of Multi-Robot SLAM using Reinforcement Learning.pdf:pdf},
	isbn = {9781467317146},
	issn = {1062922X},
	journal = {Conference Proceedings - IEEE International Conference on Systems, Man and Cybernetics},
	keywords = {SLAM,dual representation,features,grid maps,map merging,reinforcement learning},
	pages = {53--60},
	title = {{Map merging of multi-robot SLAM using reinforcement learning}},
	year = {2012}
}
@article{Carpin2008c,
	abstract = {We present a recently developed algorithm for merging multiple occupancy grid maps computed by multiple robots independently exploring a shared indoor environment. The algorithm exploits the well known Hough transform in a novel way in order to produce a set of ranked roto-translations aimed to overlap the partial maps provided as input. In this paper, after having briefly summarized such method, we investigate the impact on the performance of different variations of the Hough transform. In particular, we are interested in determining the repercussions in terms of accuracy and computational time when only a subset of points is used to compute the transformation. Results are analyzed while merging maps produced by two robots exploring an indoor environment, and also using public available data sets. It turns out that the proposed method is robust and positively influenced by the use of more refined approaches to compute the Hough transform.},
	author = {Carpin, Stefano},
	doi = {10.1109/IROS.2008.4650616},
	file = {:C$\backslash$:/Google Drive/My Documents/Varsity/Masters/MSc-2017-2018/MSc-2017-2018 Write-up/Papers/Pose Estimation/Merging Maps via Hough Transform.pdf:pdf},
	isbn = {9781424420582},
	journal = {2008 IEEE/RSJ International Conference on Intelligent Robots and Systems (IROS)},
	pages = {1878--1883},
	title = {{Merging maps via hough transform}},
	year = {2008}
}
